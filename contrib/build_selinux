#!/bin/bash -x
#GOOS=linux GOARCH=amd64 go build -o geard.linux64 geard/main.go
rm -rf $GOPATH/pkg
rm -rf $GOPATH/bin
go build -tags 'selinux' -o geard.local geard/main.go
go get -tags 'selinux' "github.com/kraman/geard-switchns"
go get -tags 'selinux' "github.com/kraman/geard-util"
/bin/cp -f $GOPATH/bin/geard-* .
/bin/cp -f geard-switchns geard-util /opt/geard/bin

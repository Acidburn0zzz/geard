[Unit]
Description=Gear Provisioning Daemon (geard)

[Service]
Type=simple
ExecStart=/usr/bin/docker run -v /var/run/dbus/system_bus_socket:/var/run/dbus/system_bus_socket -v /var/lib/containers:/var/lib/containers -p 2223:8080 -a stderr -a stdout ccoleman/geard:latest

[Install]
WantedBy=multi-user.target
